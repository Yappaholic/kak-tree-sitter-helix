set-face global ts_operator "blue+F"
set-face global ts_punctuation_bracket "blue+F"
set-face global ts_punctuation_delimiter "white+Fb"
set-face global ts_string "green+F"
set-face global ts_tag "cyan+F"
set-face global ts_type "blue+F"
set-face global ts_variable "cyan+F"
set-face global ts_variable_parameter "white+F"
set-face global ts_markup_bold "%opt{orange}+Fb"
set-face global ts_markup_italic "magenta+Fi"
set-face global ts_markup_strikethrough "+Fs"
