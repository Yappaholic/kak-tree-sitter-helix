declare-option str yellow 'rgb:D5B06B'
declare-option str blue 'rgb:519FDF'
declare-option str red 'rgb:D05C65'
declare-option str purple 'rgb:B668CD'
declare-option str green 'rgb:7DA869'
declare-option str gold 'rgb:D19A66'
declare-option str cyan 'rgb:46A6B2'
declare-option str white 'rgb:ABB2BF'
declare-option str black 'rgb:16181A'
declare-option str light_black 'rgb:2C323C'
declare-option str gray 'rgb:252D30'
declare-option str faint_gray 'rgb:ABB2BF'
declare-option str light_gray 'rgb:636C6E'
declare-option str linenr 'rgb:282C34'
set-face global ts_attribute "yellow+F"
set-face global ts_comment "bright-gray+Fi"
set-face global ts_constant "%opt{gold}+F"
set-face global ts_constant_numeric "%opt{gold}+F"
set-face global ts_constant_character_escape "%opt{gold}+F"
set-face global ts_constructor "blue+F"
