set-face global ts_label "%opt{purple}+F"
set-face global ts_namespace "%opt{purple}+F"
set-face global ts_operator "white+F"
set-face global ts_keyword_operator "white+F"
set-face global ts_special "blue+F"
set-face global ts_string "green+F"
set-face global ts_type "yellow+F"
set-face global ts_variable "white+F"
